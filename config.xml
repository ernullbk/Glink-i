<?xml version='1.0' encoding='utf-8'?>
<widget id="ir.hrlink.app" version="5.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HRLink</name>
    <description>Snappfood Token Handler</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <access origin="*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>
